@article{Deary2003,
abstract = {Discusses problems in intelligence research. First, it is hard to get far in discussing cognitive ability differences. There are too few representative samples in research on cognitive ability differences. Researchers can invest too much in the names given to constructs. The research area has been slow to realise that overall reaction times and their variabilities correlate better with psychometric intelligence than do cognitive components extracted from reaction time procedures. For many psychologists, a test measures a construct because they say it does. People might guffaw, but brain size really does correlate significantly with psychometric intelligence. Part of cognitive ageing is the slowing of the brain's 'processing speed', but what this processing speed comprises is not known. There is too little appreciation that so-called genetic studies can do far more than provide heritability estimates. Four behavioural tendencies were identified among some intelligence researchers that might limit progress. To criticise researchers' behaviours within one's research area and also to accuse observers of failing to appreciate the area's achievements attracts dudgeon from both sides.},
author = {Deary, Ian J.},
journal = {The Psychologist},
keywords = {behavioural tendencies,brain size,cognitive ability differences,cognitive ageing,intelligence research,psychometric intelligence,reaction times,researchers,researchers behaviours},
mendeley-tags = {behavioural tendencies,brain size,cognitive ability differences,cognitive ageing,intelligence research,psychometric intelligence,reaction times,researchers,researchers behaviours},
pages = {534--537},
title = {{Ten things I hated about intelligence research}},
volume = {16},
year = {2003}
}
@article{Salthouse2011c,
abstract = {There are many reports of relations between age and cognitive variables and of relations between age and variables representing different aspects of brain structure and a few reports of relations between brain structure variables and cognitive variables. These findings have sometimes led to inferences that the age-related brain changes cause the age-related cognitive changes. Although this conclusion may well be true, it is widely recognized that simple correlations are not sufficient to warrant causal conclusions, and other types of correlational information, such as mediation and correlations between longitudinal brain changes and longitudinal cognitive changes, also have limitations with respect to causal inferences. These issues are discussed, and the existing results on relations of regional volume, white matter hyperintensities, and diffusion tensor imaging measures of white matter integrity to age and to measures of cognitive functioning are reviewed. It is concluded that at the current time the evidence that these aspects of brain structure are neuroanatomical substrates of age-related cognitive decline is weak. The final section contains several suggestions concerning measurement and methodology that may lead to stronger conclusions in the future.},
